from collections import defaultdict
import sys
def init():
    return "no value"
if __name__ == "__main__":
    # 3 --> if we give a value in this argument a sa int so if dict value is not there then it will asiign a zero 
    # 2 --> if str then "" is there.
    # 1 --> we assign a string using callback function
    d = defaultdict(int)
    print("when tghere is a int:",d[337])
    e = defaultdict(str)
    print("when there is a string",e["66"])
    if e["db"] == "":
        print("yes null")
    f = defaultdict(init)
    print("this is for a callback back function :",f["f"] )
    
    
    # max int and min int concept for sorting
    print(-sys.maxsize)
